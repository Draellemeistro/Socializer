<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="351.0" prefWidth="240.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.group.p2_socializer.UserLogIn.LoginController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TextField fx:id="username"/>
   <PasswordField fx:id="password"/>
    <Label fx:id="result" />
    <Button alignment="CENTER" onAction="#onLoginButtonClick" prefHeight="47.0" prefWidth="70.0" text="Login" />
    <Button alignment="CENTER" onAction="#onRegisterButtonClick" prefHeight="47.0" prefWidth="70.0" text="Register!" />

</VBox>
